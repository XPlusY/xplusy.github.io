/* 
  Name:			Rotate In
  Type:			Entrance
  Description:	Rotate in Effect
  Version:		2.0	
  locale:		{"zh-Hans":"旋转进入", "en-US":"Rotate In"}
 */
 
@keyframes rotateIn {
  0% {
    transform-origin: center;
    transform: rotate3d(0, 0, 1, -200deg);
    opacity: 0;
  }

  100% {
    transform-origin: center;
    transform: none;
    opacity: 1;
  }
}

.rotateIn {
  animation-name: rotateIn;
}